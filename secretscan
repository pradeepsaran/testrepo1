# Append a fake AWS access key to a new file
echo "AWS_ACCESS_KEY_ID=AKIAIOSFODNN7EXAMPLE" >> fake_secret.txt

# Add the new file to the staging area
git add fake_secret.txt

# Commit the file with a message
git commit -m "Testing secret scanning feature"

# Push the commit to your GitHub repository
# (Assuming your repository is properly initialized and remote is set)
git push
